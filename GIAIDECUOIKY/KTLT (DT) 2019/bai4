#include<bits/stdc++.h>
using namespace std;
 
void batBit(int &n,int k){
	n =  n|(0x1<<k);
}
void tatBit(int &n,int k){
	n = n&(~(0x1<<k));
} 
//29122 21
int layBit(int n,int k)
{
	return (n>>k)&0x1;
}
int main(){
	int n,k; cin>>n>>k;
	int a[1000],size = 0;
	for(int i =31;i>=31-k+1;i--)
	{
		a[size] = layBit(n,i);
		size++;
	}
	n = n <<k;
	size = 0;
	for(int i = k-1;i>=0;i--)
	{
		int bit = a[size];
			if(bit == 1){
				batBit(n,i); 
			}
			else if(bit==0)
				tatBit(n,i);
		size++;
	}
	cout<<n;

}
